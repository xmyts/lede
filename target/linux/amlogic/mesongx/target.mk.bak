# 文件：target/linux/amlogic/mesongx/target.mk
ARCH:=aarch64
SUBTARGET:=mesongx
BOARDNAME:=Amlogic Meson GX/G12B (64-bit)
FEATURES+=64bit

CPU_TYPE:=cortex-a73
CPU_SUBTYPE:=cortex-a53
TARGET_CFLAGS += -mtune=cortex-a73.cortex-a53 -march=armv8.2-a

# 关键：明确设置内核版本，与主Makefile的mesongx_KERNEL_PATCHVER一致
KERNEL_PATCHVER:=5.15